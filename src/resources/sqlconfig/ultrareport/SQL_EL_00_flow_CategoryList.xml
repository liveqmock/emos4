<sql-group>
	<doc>
		<author>作者</author> 
		<date>时间</date> 
		<description>Sql 描述</description> 		
	</doc>
	<sqlquery name="SQL_EL_00_flow_CategoryList" >
		<select>
			<![CDATA[
				 select /*COUNT*/*/*COUNT*/
          from (select distinct decode(#flowFlag#,
                                       '1',
                                       base.C650000001,
                                       '2',
                                       c650000001||'.'||c650000002,
                                       '') as item,
                                decode(#flowFlag#,
                                       '1',
                                       substr(c650000005, 1, 3),
                                       '2',
                                       c650000005,
                                       '') ccode
                  from T461 base where 1=1  $customwhere$ )
        /*ORDERBY*/ order by /*ORDERBY*/ ccode
				
			]]>
		</select>
		<customwhere name="customwhere" prepend="and" >
			<field prepend="and" operator="="  colname="substr(c650000005,1,3)"  value="#item1#" />
		</customwhere>
	</sqlquery>
</sql-group>