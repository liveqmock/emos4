<struts>
	<package name="/process" extends="eoms" namespace="/process">
		<action name="*" method="{1}" class="processAction">
			<result name="custom">/workflow/jsp/${baseSchema}/${target}.jsp</result>
			<result name="close">/workflow/jsp/common/close.jsp</result>
		</action>
	</package>

</struts>
